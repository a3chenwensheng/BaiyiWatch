<resources>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallClipRotatePulse" value="3" />
            <flag name="BallBeat" value="17" />
            <flag name="LineScalePulseOut" value="18" />
        </attr>
        <attr name="indicator_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="suitlines">
        <attr name="xySize" format="float" />
        <attr name="xyColor" format="reference|color" />
        <attr name="lineType">
            <enum name="curve" value="0" />
            <enum name="segment" value="1" />
        </attr>
        <attr name="lineStyle">
            <enum name="solid" value="0" />
            <enum name="dashed" value="1" />
        </attr>
        <attr name="needEdgeEffect" format="boolean" />
        <attr name="colorEdgeEffect" format="reference|color" />
        <attr name="needClickHint" format="boolean" />
        <attr name="colorHint" format="reference|color" />
        <attr name="maxOfVisible" format="integer" />
        <attr name="countOfY" format="integer" />
    </declare-styleable>

    <declare-styleable name="Battery">
        <attr name="batteryOrientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="batteryColor" format="color"/>
        <attr name="batteryPower" format="integer"/>
    </declare-styleable>

</resources>